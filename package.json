{
  "name": "cannabis-consultant",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "start:prod": "tsx server.js",
    "worker": "tsx src/workers/ai-worker.js",
    "deploy": "antigravity deploy"
  },
  "antigravity": {
    "runtime": "nodejs18.x",
    "region": "us-west1",
    "memory": "1GB",
    "maxInstances": 10,
    "environment": "production",
    "secrets": [
      "GEMINI_API_KEY",
      "SUPABASE_URL",
      "SUPABASE_ANON_KEY",
      "DATABASE_URL"
    ],
    "routes": [
      {
        "match": "/api/*",
        "destination": "server"
      },
      {
        "match": "/*",
        "destination": "static"
      }
    ]
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.71.2",
    "@capacitor/android": "^8.0.0",
    "@capacitor/cli": "^8.0.0",
    "@capacitor/core": "^8.0.0",
    "@capacitor/ios": "^8.0.0",
    "@capacitor/local-notifications": "^8.0.0",
    "@google/generative-ai": "^0.24.1",
    "@neondatabase/serverless": "^1.0.2",
    "@prisma/adapter-neon": "^7.1.0",
    "@prisma/client": "^7.1.0",
    "@react-google-maps/api": "^2.20.7",
    "@supabase/supabase-js": "^2.86.2",
    "@vercel/analytics": "^1.6.1",
    "bcryptjs": "^3.0.3",
    "compression": "^1.7.5",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.21.1",
    "framer-motion": "^12.23.25",
    "i18next": "^25.7.2",
    "i18next-browser-languagedetector": "^8.2.0",
    "ioredis": "^5.8.2",
    "jsonwebtoken": "^9.0.3",
    "lucide-react": "^0.556.0",
    "openai": "^6.10.0",
    "pg": "^8.16.3",
    "posthog-js": "^1.306.1",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-helmet-async": "^2.0.5",
    "react-i18next": "^16.4.0",
    "react-markdown": "^10.1.0",
    "react-router-dom": "^7.10.1",
    "recharts": "^3.5.1",
    "socket.io-client": "^4.8.1",
    "tiktoken": "^1.0.22",
    "tsx": "^4.19.2",
    "ws": "^8.18.3",
    "zod": "^4.1.13",
    "zustand": "^5.0.9"
  },
  "devDependencies": {
    "@capacitor/assets": "^3.0.5",
    "@eslint/js": "^9.39.1",
    "@tailwindcss/postcss": "^4.1.17",
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "@types/ws": "^8.18.1",
    "@vitejs/plugin-react": "^5.1.1",
    "autoprefixer": "^10.4.22",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "postcss": "^8.5.6",
    "prisma": "^7.1.0",
    "tailwindcss": "^4.1.17",
    "typescript": "^5.9.3",
    "vercel": "^25.2.0",
    "vite": "^7.2.4",
    "vite-plugin-pwa": "^1.2.0"
  }
}
